{
  "name": "todo-api",
  "version": "0.1.0",
  "description": "App to store notes",
  "main": "app.js",
  "scripts": {
    "test": "export NODE_ENV=test && mocha -r ts-node/register 'test/**/*.test.js' --exit",
    "test-coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "test-watch": "nodemon --exec \"npm test\"",
    "build": "rm -rf dist && tsc",
    "start": "node src/server.js",
    "start:watch": "nodemon --watch src src/server.js"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "6.4.1"
  },
  "author": "moser.sts",
  "license": "ISC",
  "dependencies": {
    "@snyk/nodejs-runtime-agent": "^1.46.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "crypto-js": "^3.1.9-1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.5",
    "ts-mongoose": "0.0.21",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.144",
    "@types/mongoose": "^5.5.28",
    "@types/node": "^12.12.5",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "expect": "^24.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "supertest": "^3.1.0",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.6.4"
  }
}
